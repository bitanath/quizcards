<li class="deck">
    <div class="card">        
        <div class="card-content">
            <span class="card-title activator grey-text text-darken-4">Question<i class="fa fa-ellipsis-v right"></i></span>
            <p>{{this.question}}</p>
        </div>
        <div class="card-reveal">
            <span class="card-title grey-text text-darken-4">Answer<i class="fa fa-close right"></i></span>
            <p>{{this.answer}}</p>
        </div>
    </div>
</li>